{
  "package": "com.meizu.template",
  "name": "template-quickapp",
  "versionName": "1.0.0",
  "versionCode": "1",
  "minPlatformVersion": "1020",
  "icon": "/assets/images/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    },
    {
      "name": "system.network"
    },
    {
      "name": "system.fetch"
    },
    {
      "name": "system.storage"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "config": {
    "designWidth": 1080,
    "logLevel": "debug"
  },
  "debugMode": true,
  "router": {
    "entry": "pages/main",
    "pages": {
      "pages/main": {
        "component": "index"
      }
    },
    "widgets": {}
  },
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "menu": true,
    "pages": {
      "pages/main": {
        "titleBarText": "示例页",
        "menu": false
      }
    }
  }
}
